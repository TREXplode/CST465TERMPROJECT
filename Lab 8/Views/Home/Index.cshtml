@model Lab8Model
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/DefaultLayout.cshtml";
}

<h2>Home Controller Index Action</h2>

<div class="container">
    <div class="row">
        <form asp-action="Index" asp-controller="Home" method="post">
            <input type="text" asp-for="@Model.outs" width="60" height="10" />
            <input type="text" asp-for="@Model.id" width="60" height="10" />
            <button type="submit" class="btn">Submit</button>

        </form>
    </div>
    <div class="row">
        <table class="table">
            <tr>
                <td>@Model.outs</td>
                <td>@Model.id</td>
            </tr>
        </table>
    </div>
</div>